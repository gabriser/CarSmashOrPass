{
    "version": 2,
    "builds": [
      {
        "src": "carsmashorpass/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "carsmashorpass/dist"
        }
      },
      {
        "src": "server",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/cars/(.*)",
        "dest": "/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "carsmashorpass/dist/$1"
      }
    ]
  }
  